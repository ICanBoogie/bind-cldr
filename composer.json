{
	"name": "icanboogie/bind-cldr",
	"type": "library",
	"description": "Binds CLDR to ICanBoogie",
	"homepage": "https://icanboogie.org/",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "https://olvlvl.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/ICanBoogie/bind-cldr/issues",
		"source": "https://github.com/ICanBoogie/bind-cldr"
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"prefer-dist": true,
	"require": {
		"php": ">=8.2",
		"icanboogie/icanboogie": "^6.0",
		"icanboogie/cldr": "^6.0"
	},
	"autoload": {
		"psr-4": {
			"ICanBoogie\\Binding\\CLDR\\": "lib"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Test\\ICanBoogie\\Binding\\CLDR\\": "tests/lib"
		}
	},
	"extra": {
		"icanboogie": {
			"config-path": "config"
		}
	},
	"scripts": {
		"post-autoload-dump": "ICanBoogie\\Autoconfig\\Hooks::on_autoload_dump"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.12",
		"phpunit/phpunit": "^11.3"
	}
}
